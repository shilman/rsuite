{
  "Tooltip": {
    "props": {
      "arrow": {
        "type": "boolean",
        "isRequired": false,
        "description": "Whether show the arrow indicator",
        "defaultValue": "true"
      },
      "children": {
        "type": "ReactNode",
        "isRequired": true,
        "description": "The content of the component."
      },
      "classPrefix": {
        "type": "string",
        "isRequired": false,
        "description": "The prefix of the component CSS class",
        "defaultValue": "tooltip"
      },
      "visible": {
        "type": "boolean",
        "isRequired": false,
        "description": "The component is visible by default"
      }
    }
  }
}