{
  "Cascader": {
    "props": {
      "appearance": {
        "type": "'default' | 'subtle'",
        "isRequired": false,
        "description": "Set picker appearance",
        "defaultValue": "default"
      },
      "block": {
        "type": "boolean",
        "isRequired": false,
        "description": "Occupy the full width of the parent container"
      },
      "caretAs": {
        "type": "ElementType",
        "isRequired": false,
        "description": "Custom component for the caret icon"
      },
      "childrenKey": {
        "type": "string",
        "isRequired": false,
        "description": "Set children key in data",
        "defaultValue": "children"
      },
      "classPrefix": {
        "type": "string",
        "isRequired": false,
        "description": "The prefix of the component CSS class",
        "defaultValue": "picker"
      },
      "cleanable": {
        "type": "boolean",
        "isRequired": false,
        "description": "Whether the selected value can be cleared",
        "defaultValue": "true"
      },
      "columnHeight": {
        "type": "number",
        "isRequired": false,
        "description": "Sets the height of the column"
      },
      "columnWidth": {
        "type": "number",
        "isRequired": false,
        "description": "Sets the width of the column"
      },
      "container": {
        "type": "HTMLElement | (() => HTMLElement)",
        "isRequired": false,
        "description": "Sets the rendering container"
      },
      "data": {
        "type": "[Option][item][]",
        "isRequired": true,
        "description": "The data of component"
      },
      "defaultValue": {
        "type": "string",
        "isRequired": false,
        "description": "Default values of the selected items"
      },
      "disabled": {
        "type": "boolean",
        "isRequired": false,
        "description": "Whether to disable the component"
      },
      "disabledItemValues": {
        "type": "string[]",
        "isRequired": false,
        "description": "Values of disabled items"
      },
      "getChildren": {
        "type": "(item: [Option][item]) => Promise&lt;[Option][item][]&gt;",
        "isRequired": false,
        "description": "Asynchronously load the children of the tree node"
      },
      "height": {
        "type": "number",
        "isRequired": false,
        "description": "The height of Dropdown",
        "defaultValue": "320"
      },
      "labelKey": {
        "type": "string",
        "isRequired": false,
        "description": "Set label key in data",
        "defaultValue": "label"
      },
      "loading": {
        "type": "boolean",
        "isRequired": false,
        "description": "Whether to display a loading state indicator",
        "defaultValue": "false"
      },
      "locale": {
        "type": "PickerLocaleType",
        "isRequired": false,
        "description": "Locale text settings"
      },
      "onChange": {
        "type": "(value:string, event) => void",
        "isRequired": false,
        "description": "Callback fired when value changes"
      },
      "onClean": {
        "type": "(event) => void",
        "isRequired": false,
        "description": "Callback fired when value is cleared"
      },
      "onClose": {
        "type": "() => void",
        "isRequired": false,
        "description": "Callback fired when component closes"
      },
      "onEnter": {
        "type": "() => void",
        "isRequired": false,
        "description": "Callback fired before the overlay transitions in"
      },
      "onEntered": {
        "type": "() => void",
        "isRequired": false,
        "description": "Callback fired after the overlay finishes transitioning in"
      },
      "onEntering": {
        "type": "() => void",
        "isRequired": false,
        "description": "Callback fired as the overlay begins to transition in"
      },
      "onExit": {
        "type": "() => void",
        "isRequired": false,
        "description": "Callback fired right before the overlay transitions out"
      },
      "onExited": {
        "type": "() => void",
        "isRequired": false,
        "description": "Callback fired after the overlay finishes transitioning out"
      },
      "onExiting": {
        "type": "() => void",
        "isRequired": false,
        "description": "Callback fired as the overlay begins to transition out"
      },
      "onOpen": {
        "type": "() => void",
        "isRequired": false,
        "description": "Callback fired when component opens"
      },
      "onSearch": {
        "type": "(search:string, event) => void",
        "isRequired": false,
        "description": "Callback function for search"
      },
      "onSelect": {
        "type": "(item: [Option][item], selectedPaths: [Option][item][], event) => void",
        "isRequired": false,
        "description": "Callback fired when an item is selected"
      },
      "open": {
        "type": "boolean",
        "isRequired": false,
        "description": "Whether the component is open"
      },
      "parentSelectable": {
        "type": "boolean",
        "isRequired": false,
        "description": "Whether parent nodes are selectable"
      },
      "placeholder": {
        "type": "ReactNode",
        "isRequired": false,
        "description": "Placeholder text",
        "defaultValue": "Select"
      },
      "placement": {
        "type": " | 'bottomStart' | 'bottomEnd' | 'topStart' | 'topEnd' | 'leftStart' | 'leftEnd' | 'rightStart' | 'rightEnd' | 'auto' | 'autoVerticalStart' | 'autoVerticalEnd' | 'autoHorizontalStart' | 'autoHorizontalEnd'",
        "isRequired": false,
        "description": "The placement of component",
        "defaultValue": "bottomStart"
      },
      "popupClassName": {
        "type": "string",
        "isRequired": false,
        "description": "Custom CSS class for the popup"
      },
      "popupStyle": {
        "type": "CSSProperties",
        "isRequired": false,
        "description": "Custom style for the popup"
      },
      "preventOverflow": {
        "type": "boolean",
        "isRequired": false,
        "description": "Prevent floating element overflow"
      },
      "renderColumn": {
        "type": "(childNodes: ReactNode, column: { items, parentItem, layer}) => ReactNode",
        "isRequired": false,
        "description": "Custom render function for column list"
      },
      "renderExtraFooter": {
        "type": "() => ReactNode",
        "isRequired": false,
        "description": "Custom render function for extra footer"
      },
      "renderSearchItem": {
        "type": "(node: ReactNode, items: [Option][item][]) => ReactNode",
        "isRequired": false,
        "description": "Custom render function for search result items"
      },
      "renderTreeNode": {
        "type": "(node: ReactNode, item: [Option][item]) => ReactNode",
        "isRequired": false,
        "description": "Custom render function for tree nodes"
      },
      "renderValue": {
        "type": "(value: string, selectedPaths: [Option][item][], selected:ReactNode) => ReactNode",
        "isRequired": false,
        "description": "Custom render function for selected items"
      },
      "searchable": {
        "type": "boolean",
        "isRequired": false,
        "description": "Whether the component is searchable",
        "defaultValue": "true"
      },
      "size": {
        "type": "'lg' | 'md' | 'sm' | 'xs'",
        "isRequired": false,
        "description": "Size of the component",
        "defaultValue": "md"
      },
      "toggleAs": {
        "type": "ElementType",
        "isRequired": false,
        "description": "Custom element for the component",
        "defaultValue": "a"
      },
      "value": {
        "type": "string",
        "isRequired": false,
        "description": "Value of the component (Controlled)"
      },
      "valueKey": {
        "type": "string",
        "isRequired": false,
        "description": "Set value key in data",
        "defaultValue": "value"
      }
    }
  }
}