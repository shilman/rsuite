        "defaultValue": "div"
      },
      "bordered": {
        "type": "boolean",
        "isRequired": false,
        "description": "Whether to display a border"
      },
      "children": {
        "type": "ReactNode",
        "isRequired": false,
        "description": "The children of the component"
      },
      "classPrefix": {
        "type": "string",
        "isRequired": false,
