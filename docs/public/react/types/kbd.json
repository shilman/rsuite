{
  "Kbd": {
    "props": {
      "children": {
        "type": "ReactNode",
        "isRequired": false,
        "description": "The content of the kbd."
      },
      "classPrefix": {
        "type": "string",
        "isRequired": false,
        "description": "The prefix of the component CSS class.",
