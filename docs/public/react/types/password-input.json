{
  "PasswordInput": {
    "props": {
      "classPrefix": {
        "type": "string",
        "isRequired": false,
        "description": "The prefix of the component CSS class.",
        "defaultValue": "password-input"
      },
      "defaultValue": {
        "type": "string",
        "isRequired": false,
        "description": "Default value (uncontrolled)"
      },
      "defaultVisible": {
        "type": "boolean",
        "isRequired": false,
        "description": "Default visibility state of the password"
      },
      "disabled": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render the component in a disabled state"
      },
      "endIcon": {
        "type": "ReactNode",
        "isRequired": false,
        "description": "End icon for the input field"
      },
      "htmlSize": {
        "type": "number",
        "isRequired": false,
        "description": "Sets the native HTML size attribute."
      },
      "onChange": {
        "type": "(value: string, event) => void",
        "isRequired": false,
        "description": "Callback function when value changes"
      },
      "onVisibleChange": {
        "type": "(visible: boolean) => void",
        "isRequired": false,
        "description": "Callback function triggered when the password visibility changes"
      },
      "readOnly": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render the component in a read-only state"
      },
      "renderVisibilityIcon": {
        "type": "(visible: boolean) => ReactNode",
        "isRequired": false,
        "description": "Custom icon for visibility toggle"
      },
      "size": {
        "type": "'lg' | 'md' | 'sm' | 'xs'",
        "isRequired": false,
        "description": "An input can have different sizes",
        "defaultValue": "md"
      },
      "startIcon": {
        "type": "ReactNode",
        "isRequired": false,
        "description": "Start icon for the input field"
      },
      "value": {
        "type": "string",
        "isRequired": false,
        "description": "Current value (controlled)"
      },
      "visible": {
        "type": "boolean",
        "isRequired": false,
        "description": "Controls whether the password is visible"
      }
    }
  },
  "PasswordStrengthMeter": {
    "props": {
      "classPrefix": {
        "type": "string",
        "isRequired": false,
        "description": "The prefix of the component CSS class.",
        "defaultValue": "password-strength-meter"
      },
      "label": {
        "type": "ReactNode",
        "isRequired": false,
        "description": "Label for the strength meter"
      },
      "level": {
        "type": "number",
        "isRequired": false,
        "description": "Password strength level"
      },
      "max": {
        "type": "number",
        "isRequired": false,
        "description": "Maximum strength level",
        "defaultValue": "4"
      }
    }
  }
}